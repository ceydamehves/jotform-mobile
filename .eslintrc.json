{
    "extends": [ "airbnb", "prettier", "plugin:node/recommended", "eslint:recommended" ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier":"error",
        "semi": ["error", "always"],
        "no-unused-vars":"warn",
        "no-console":"off",
        "func-names":"off",
        "no-process-exit":"off",
        "object-shorthand":"off",
        "class-methods-use-this": "off"
    },
    "parserOptions": {
        "sourceType": "module"
    }
}
